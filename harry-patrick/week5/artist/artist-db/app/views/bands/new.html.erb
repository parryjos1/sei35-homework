<h1>Add A band</h1>
<form action="<%= bands_path %>" method="post">
  <label>
    Name:
    <input type="text" name="name">
  </label>
  <label>
    Genre:
    <input type="text" name="genre">
  </label>
  <label>
    Image:
    <input type="text" name="image">
  </label>

  <input type="submit" name="Add">
</form>
