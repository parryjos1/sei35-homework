<h2>Add New Musician!</h2>
<form action="/musicians" method="post">

  <label>
    Name:
    <input type="text" name="name">
  </label>
  <br>

  <label>
    Year Active:
    <input type="text" name="year_active">
  </label>
  <br>

  <label>
    Hometown:
    <input type="text" name="hometown">
  </label>
  <br>

  <label>
    Labels:
    <input type="text" name="labels">
  </label>
  <br>

  <label>
    Occupations:
    <input type="text" name="occupations">
  </label>
  <br>

  <label>
    Still Active:
    <input type="text" name="still_active">
  </label>
  <br>

  <label>
    Image URL:
    <input type="text" name="image_url">
  </label>
  <br>
  
  <input type="submit" value="Add New Musician">
</form>
